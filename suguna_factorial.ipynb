   "source": [
    "def factorial(num):\n",
    "    if num==0 or num==1:\n",
    "        return 1\n",
    "    else:\n",
    "        return num*factorial(num-1)\n",
    "num=int(input(\"Enter the number to find the factorial\"))\n",
    "print(\"The factorial of \",num,\"is\",factorial(num))\n",
    "\n",
